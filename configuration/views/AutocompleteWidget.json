{
  "name": "AutocompleteWidget",
  "type": "view",
  "public": true,
  "styles": [
    "match_parent"
  ],
  "elements": [
    {
      "type": "container",
      "nestedScreen": "AppbarBlock"
    },
    {
      "type": "view",
      "orientation": "vertical",
      "scrollable": true,
      "align": "center_top",
      "styles": [
        "match_parent"
      ],
      "elements": [
        {
          "id": "openDocLabel",
          "type": "label",
          "value": "{docLink}",
          "styles": [
            "label_link"
          ]
        },
        {
          "type": "label",
          "align": "center",
          "value": "{autocompleteDescription}"
        },
        {
          "type": "view",
          "orientation": "horizontal",
          "styles": [
            "width_full",
            "padding_all_16",
            "margin_vertical_8",
            "space_between",
            {
              "self": [
                {
                  "borderWidth": 1,
                  "borderRadius": 5,
                  "backgroundColor": "#f3f3f3"
                }
              ]
            }
          ],
          "elements": [
            {
              "type": "view",
              "elements": [
                {
                  "type": "checkbox",
                  "id": "visibilityCheckbox",
                  "title": "visibility",
                  "value": true
                },
                {
                  "type": "checkbox",
                  "id": "isReadOnlyCheckbox",
                  "title": "readonly"
                },
                {
                  "type": "checkbox",
                  "id": "isLoadingCheckbox",
                  "title": "isLoading"
                }
              ]
            },
            {
              "type": "view",
              "align": "center_top",
              "elements": [
                {
                  "id": "setDataButton",
                  "type": "button",
                  "title": "set data",
                  "styles": [
                    "margin_vertical_8"
                  ]
                },
                {
                  "id": "setValueButton",
                  "type": "button",
                  "title": "set value",
                  "styles": [
                    "margin_vertical_8"
                  ]
                },
                {
                  "id": "clearValueButton",
                  "type": "button",
                  "title": "clear value",
                  "styles": [
                    "margin_vertical_8"
                  ]
                }
              ]
            },
            {
              "type": "view",
              "align": "center_top",
              "elements": [
                {
                  "id": "setErrorButton",
                  "type": "button",
                  "title": "set error",
                  "styles": [
                    "margin_vertical_8"
                  ]
                },
                {
                  "id": "clearErrorButton",
                  "type": "button",
                  "title": "clear error",
                  "styles": [
                    "margin_vertical_8"
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "label",
          "value": "Flat popover without floating title",
          "styles": [
            {
              "self": [
                {
                  "marginTop": 32
                }
              ]
            }
          ]
        },
        {
          "type": "button",
          "id": "FPWFT_isSuggestionsOpenedButton",
          "title": "isSuggestionsOpened"
        },
        {
          "type": "view",
          "orientation": "horizontal",
          "styles": [
            "margin_vertical_8"
          ],
          "elements": [
            {
              "id": "FPWFT_popupButton",
              "type": "button",
              "title": "popup"
            },
            {
              "id": "FPWFT_getAttrsButton",
              "type": "button",
              "title": "get attrs"
            }
          ]
        },
        {
          "type": "container",
          "nestedScreen": "FlatPopoverWithoutFloatingTitleAutocomplete"
        },
        {
          "type": "label",
          "value": "Flat fixed with floating title",
          "styles": [
            {
              "self": [
                {
                  "marginTop": 32
                }
              ]
            }
          ]
        },
        {
          "type": "view",
          "orientation": "horizontal",
          "styles": [
            "margin_vertical_8"
          ],
          "elements": [
            {
              "id": "FFFT_popupButton",
              "type": "button",
              "title": "popup"
            },
            {
              "id": "FFFT_getAttrsButton",
              "type": "button",
              "title": "get attrs"
            }
          ]
        },
        {
          "type": "container",
          "nestedScreen": "FlatFixedWithFloatingTitleAutocomplete"
        },
        {
          "type": "label",
          "value": "Outlined popover without floating title",
          "styles": [
            {
              "self": [
                {
                  "marginTop": 32
                }
              ]
            }
          ]
        },
        {
          "type": "button",
          "id": "OPWFT_isSuggestionsOpenedButton",
          "title": "isSuggestionsOpened"
        },
        {
          "type": "view",
          "orientation": "horizontal",
          "styles": [
            "margin_vertical_8"
          ],
          "elements": [
            {
              "id": "OPWFT_popupButton",
              "type": "button",
              "title": "popup"
            },
            {
              "id": "OPWFT_getAttrsButton",
              "type": "button",
              "title": "get attrs"
            }
          ]
        },
        {
          "type": "container",
          "nestedScreen": "OutlinedPopoverWithoutFloatingTitleAutocomplete"
        },
        {
          "type": "label",
          "value": "Outlined fixed with floating title",
          "styles": [
            {
              "self": [
                {
                  "marginTop": 32
                }
              ]
            }
          ]
        },
        {
          "type": "view",
          "orientation": "horizontal",
          "styles": [
            "margin_vertical_8"
          ],
          "elements": [
            {
              "id": "OFFT_popupButton",
              "type": "button",
              "title": "popup"
            },
            {
              "id": "OFFT_getAttrsButton",
              "type": "button",
              "title": "get attrs"
            }
          ]
        },
        {
          "type": "container",
          "nestedScreen": "OutlinedFixedWithFloatingTitleAutocomplete"
        }
      ]
    }
  ]
}